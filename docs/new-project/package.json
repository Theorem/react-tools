    "format-verify": "prettier --check 'src/**/*.{css,html,js,json,jsx,md,ts,tsx}' '*.{css,html,js,json,jsx,md,ts,tsx}'",
    "start": "NODE_ENV=development webpack-dev-server --history-api-fallback --hot --host 127.0.0.1 --port 3123",
